<div class="col-xs-12 col-sm-6 col-md-4 col-lg-4">
    <div class="panel panel-primary" style="cursor:pointer" onclick="openEvent(<%=this.id%>)">
    	<div class="panel-heading">
			<h3 class="panel-title"><%=this.category%></h3>
		</div>
		<div class="panel-body">
			<p class="text-center"><%=this.subcategory%></p>
			<br/>	
			<p class="text-center"><%=this.event%></p>
			<br />
			<p class="text-center"><%=this.date%></p>
		</div>
	</div>
</div>
